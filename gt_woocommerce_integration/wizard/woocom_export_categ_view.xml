<openerp>		
	<data>	
	<record model="ir.ui.view" id="view_export_woocom_categ_wiz_form">
			<field name="name">view.export.woocom.categ.wiz.form</field>
			<field name="model">export.categ.wiz</field>
			<field name="arch" type="xml">
				<form>
				<group>
				<field name="select_instance" />
						<field name="shop_ids" />
				</group>
					<footer>
						<button string='Export' name="export_to_woocom" type="object"
							class="btn-primary" />
