		<act_window
            id="action_export_woocom_categ_wiz"
            name="Export Woocom Category"
            res_model="export.categ.wiz"
            src_model="product.category"
            view_mode="form"
            multi="True"
            target="new"
            key2="client_action_multi"
        />
</data>
</openerp>